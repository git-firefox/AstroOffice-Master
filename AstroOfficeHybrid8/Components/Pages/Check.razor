@page "/products"
@page "/products/{CategorySno}/{CategoryName}"

<AstroOfficeSharedComponents.ViewProducts CategoryName="CategoryName" CategorySno="CategorySno" CurrentPage="CurrentPage"></AstroOfficeSharedComponents.ViewProducts>

@code{
    [Parameter]
    public string? CategorySno { get; set; } = null;
    [Parameter]
    public string? CategoryName { get; set; } = null;

    [Parameter]
    public int CurrentPage { get; set; } = 1;

}