@page "/products"
@page "/products/{CategorySno}/{CategoryName}"

<PageLayout Title="Shop">
    <ChildContent>
        <AstroOfficeWeb.Components.ProductComponents.ViewProducts CategoryName="@CategoryName" CategorySno="@CategorySno" CurrentPage="@CurrentPage"></AstroOfficeWeb.Components.ProductComponents.ViewProducts>
    </ChildContent>
</PageLayout>

@code{
    [Parameter]
    public string? CategorySno { get; set; } = null;
    [Parameter]
    public string? CategoryName { get; set; } = null;

    [Parameter]
    public int CurrentPage { get; set; } = 1;

}