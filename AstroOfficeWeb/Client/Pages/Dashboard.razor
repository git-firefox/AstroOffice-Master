                                                            }
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        }
                                    </div>
                                </div>
                            </div>

                            @* Date Finder *@
                            <div class="tab-pane fade" id="birthdetails-tabs-date-finder" role="tabpanel" aria-labelledby="birthdetails-tabs-date-finder-tab">
                                <div class="row">
                                    <div class="col-4">
                                        <div class="date-finder">
                                            <EditForm Model="DateFinder">
                                                <div class="row mt-2">
                                                    <div class="col">
                                                        <div class="d-flex justify-content-between">
                                                            <label> Choose Occassion</label>

                                                            <div class="input-group occasion-selection">


                                                                <input class="form-control" type="text" name="product" @bind="DateFinder!.Occassion" list="productName" />
                                                                <div class="input-group-append">
                                                                    <InputSelect @bind-Value="DateFinder!.Occassion" class="form-control custom-select" aria-label="Section of Occassion">
                                                                        @foreach (var item in DateFinder.OccassionList)
                                                                        {
                                                                            <option value="@item.Text">@item.Text</option>
                                                                        }
                                                                    </InputSelect>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="row mt-2">
                                                    <div class="col">
                                                        <div class="d-flex justify-content-between">
                                                            <div class="form-check">
                                                                <InputCheckbox id="sahasane" class="form-check-input" @bind-Value="DateFinder!.Sahasane" />
                                                                <label class="form-check-label" for="sahasane">Sahasane</label>
                                                            </div>
                                                            <div class="form-check">
                                                                <InputCheckbox id="fullMatch" class="form-check-input" @bind-Value="DateFinder!.FullMatch" />
                                                                <label class="form-check-label" for="fullMatch">Full match</label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="row mt-3">
                                                    <div class="col">
                                                            <h6 class="font-weight-bold">Choose Period</h6>
                                                            <div class="row">
                                                                <InputRadioGroup Name="DateFinderChoosePeriod" @bind-Value="DateFinder!.Period">

                                                                    <div class="col">
                                                                        <div class="form-check">
                                                                            <InputRadio Value="Period.Maha" class="form-check-input" id="rbMaha" />
                                                                            <label class="form-check-label" for="rbMaha">
                                                                                Maha
                                                                            </label>
                                                                        </div>
                                                                    </div>

                                                                    <div class="col">
                                                                        <div class="form-check">
                                                                            <InputRadio Value="Period.Antar" class="form-check-input" id="rbAntar" />
                                                                            <label class="form-check-label" for="rbAntar">
                                                                                Antar
                                                                            </label>
                                                                        </div>
                                                                    </div>

                                                                    <div class="col">
                                                                        <div class="form-check">
                                                                            <InputRadio Value="Period.Pryan" class="form-check-input" id="rbPryan" />
                                                                            <label class="form-check-label" for="rbPryan">
                                                                                Pryan
                                                                            </label>
                                                                        </div>
                                                                    </div>
                                                                </InputRadioGroup>
                                                            </div>
                                                    </div>
                                                </div>

                                                <div class="row mt-2">
                                                    <div class="col">
                                                            <div class="row">
                                                                <InputRadioGroup Name="DateFinderDashaRadioButtons" @bind-Value="DateFinder!.Dasha">
                                                                    <div class="col">
                                                                        <div class="form-check">
                                                                            <InputRadio Value="Dasha.Dasha" class="form-check-input" id="rbDasha" />
                                                                            <label class="form-check-label" for="rbDasha">
                                                                                Dasha
                                                                            </label>
                                                                        </div>
                                                                    </div>

                                                                    <div class="col">
                                                                        <div class="form-check">
                                                                            <InputRadio Value="Dasha.NakSwami" class="form-check-input" id="rbNakswami" />
                                                                            <label class="form-check-label" for="rbNakswami">
                                                                                Nak Swami
                                                                            </label>
                                                                        </div>
                                                                    </div>

                                                                    <div class="col">
                                                                        <div class="form-check">
                                                                            <InputRadio Value="Dasha.Both" class="form-check-input" id="rbForBoth" />
                                                                            <label class="form-check-label" for="rbForBoth">
                                                                                Both
                                                                            </label>
                                                                        </div>
                                                                    </div>
                                                                </InputRadioGroup>
                                                            </div>
                                                    </div>
                                                </div>

                                                <div class="row mt-3">
                                                    <div class="col">
                                                        <div class="d-flex flex-row-reverse">
                                                            <button type="button" class="btn btn-custom" @onclick="OnClick_BtnShow_TabDateFinder">Show</button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </EditForm>
                                        </div>
                                    </div>

                                    <div class="col-8">
                                        <div class="date-finder">
                                            <EditForm Model="DateFinder">
                                            <InputTextArea class="form-control" rows="13" Placeholder="Date Lists ..." @bind-Value="DateFinder.DateList" />
                                        </EditForm>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            @* Best Kundali Dates *@
                            <div class="tab-pane fade " id="birthdetails-tabs-best-kundali-dates" role="tabpanel" aria-labelledby="birthdetails-tabs-best-kundali-dates-tab">
                                <div class="row">
                                    <div class="col-4">

                                        <div class="date-finder">
                                        <EditForm Model="BestKundaliDates">
                                            <div class="row">
                                                <div class="col">
                                                        <div class="row align-items-end mt-2">
                                                            <div class="col-12">
                                                                <h6 class="font-weight-bold">Starting Date and Time</h6>
                                                            </div>
                                                            <div class="col">
                                                                <div>
                                                                    <InputDate @bind-Value="BestKundaliDates.Pick_start_date" class="form-control" style="border: 1px solid #ccc; padding: 2px; height: 30px;" />
                                                                </div>
                                                            </div>
                                                            <div class="col">
                                                                <div>
                                                                    <input type="time" @bind="BestKundaliDates.StartingTime" class="form-control" step="1" style="border: 1px solid #ccc; padding: 2px; height: 30px;" />
                                                                </div>
                                                            </div>
                                                        </div>
                                                </div>
                                            </div>
                                            <div class="row mt-2">
                                                <div class="col">
                                                        <div class="row align-items-end mt-2">
                                                            <div class="col-12">

                                                                    <h6 class="font-weight-bold">Ending Date and Time</h6>
                                                            </div>
                                                            <div class="col">
                                                                <div>
                                                                    <InputDate @bind-Value="BestKundaliDates.Pick_end_date" class="form-control" style="border: 1px solid #ccc; padding: 2px; height: 30px;" />
                                                                </div>
                                                            </div>
                                                            <div class="col">
                                                                <div>
                                                                    <input type="time" @bind="BestKundaliDates.EndingTime" class="form-control" step="1" style="border: 1px solid #ccc; padding: 2px; height: 30px;" />
                                                                </div>
                                                            </div>
                                                        </div>
                                                </div>
                                            </div>
                                            <div class="row mt-3">
