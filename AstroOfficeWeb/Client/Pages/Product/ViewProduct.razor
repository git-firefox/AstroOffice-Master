@page "/product/{Sno:long}/{Name}"

<FluidPageLayout Title="Manage Orders">
    <ChildContent>
        <AstroOfficeWeb.Components.ProductComponents.ViewProduct ProductSno="Sno" ProductName="@Name" />
    </ChildContent>
</FluidPageLayout>

@code
{
    [Parameter]
    public long Sno { get; set; }

    [Parameter]
    public string? Name { get; set; }
}