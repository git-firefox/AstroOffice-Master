@page "/products"
@page "/products/{Sno}/{Name}"
@* @page "/products"
@page "/products/{CategorySno}/{CategoryName}" *@
@* @page "/products/{CurrentPage:int}" *@

@* @page "/products/{CurrentPage:int}" *@

<FluidPageLayout Title="Shop">
    <ChildContent>
        <AstroOfficeWeb.Components.ProductComponents.ViewProducts CategorySno="@Sno" CategoryName="@Name" CurrentPage="Page" />
    </ChildContent>
</FluidPageLayout>

@code {
    [Parameter]
    public string? Sno { get; set; } = null;

    [Parameter]
    public string? Name { get; set; } = null;

    [Parameter]
    public int Page { get; set; } = 1;
}