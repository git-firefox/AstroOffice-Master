    </div>

    <div class="card-body">
        <div class="tab-content" id="custom-tabs-four-tabContent">
            <div class="tab-pane fade" id="custom-tabs-basic-details" role="tabpanel" aria-labelledby="custom-tabs-basic-details-tab">

                <div class="card-body p-0">
                    <div class="table-container">
                        <table class="table table-sm table table-bordered">
                            @*  <thead class="p-0 bg-da-yellow">
                            <tr>
                            <th>Planet</th>
                            <th>Period</th>
                            <th>Signi(Self/Nak)</th>
                            </tr>
                            </thead> *@
                            <tbody class="data-loader">


                                <tr>
                                    <td>Name</td>
                                    <td>Shiva</td>

                                </tr>
                                <tr>
                                    <td>DOB</td>
                                    <td>01-Jan-1990</td>

                                </tr>
                                <tr>
                                    <td>Time Of Birth</td>
                                    <td>10:12 pm</td>

                                </tr>
                                <tr>
                                    <td>Country</td>
                                    <td>India</td>

                                </tr>
                                <tr>
                                    <td>Birth Place</td>
                                    <td>Delhi</td>

                                </tr>
                                <tr>
                                    <td>Longtitude</td>
                                    <td>77.22</td>

                                </tr>
                                <tr>
                                    <td>Time Zone</td>
                                    <td>83.75</td>

                                </tr>
                                <tr>
                                    <td>Gender</td>
                                    <td>Male</td>

                                </tr>

                            </tbody>
                        </table>
                    </div>
                </div>              

            </div>
            <div class="tab-pane fade" id="custom-tabs-chart" role="tabpanel" aria-labelledby="custom-tabs-chart-tab">

                <div>


                    <div class="card-header p-0 mb-2">
                        <h2 class="font-weight">Kundali</h2>
                    </div>
                    <div class="card-body">

                        <div class="row">
                            <div class="col">
                                <button type="button" class="btn btn-outline-warning btn-sm" title="Lagan"> Lagan </button>
                            </div>
                            <div class="col">
                                @* <button type="button" class="btn btn-block btn-custom btn-sm" title="Chandra"> Chandra </button> *@
                                <button type="button" class="btn btn-outline-warning btn-sm" title="Chandra"> Chandra </button>
                            </div>
                            <div class="col">
                                <button type="button" class="btn btn-outline-warning btn-sm" title="Gochar"> Gochar </button>
                            </div>
                            <div class="col">
                                <button type="button" class="btn btn-outline-warning btn-sm" title="Varsh"> Varsh </button>
                            </div>
                        </div>

                        <div class="row justify-content-end mt-2">
                            <div class="col-2">
                                @if (isNumVarshVisible)
                                {
                                    <input type="number" id="1" class="form-control form-control-sm" min="1" max="120" />
                                }
                                else
                                {

                                    <input type="number" class="form-control form-control-sm" style="opacity:0" min="1" max="120" />
                                }
                            </div>
                        </div>
                        <div class="data-loader mt-2">
                            <div class="overlay"></div>
                            <div>
                                <img class="img-fluid text-center" src="data:image/png;base64," />
                            </div>

                        </div>


                    </div>

                    @* chart table *@

                    <div class="card-header p-0 mb-2">
                        <h2 class="font-weight">Chart</h2>
                    </div>
                    <div class="card-body p-0">
                        <div class="table-container">
                            <table class="table table-sm table table-bordered">
                                <thead class="p-0 bg-da-yellow">
                                    <tr>
                                        <th>House</th>
                                        <th>RL-NL-SL-SSL</th>
                                        <th>Nak Signi</th>
                                        <th>Sub Signi</th>
                                    </tr>
                                </thead>
                                <tbody class="data-loader">

                                    @for (int i = 0; i < 12; i++)
                                    {
                                        <tr>
                                            <td>@(i + 1)</td>
                                            <td>सूर्य--शुक्र--शुक्र--राहु</td>
                                            <td>| 11 | -6 7 | 11 | -6 7</td>
                                            <td>| 11 | -6 7 | -8 | +10 3</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <div class="col-lg-5">
                        <div class="table-container">
                            <div class="chart-table table-responsive">
                                <table class="table table-bordered table table-sm">
                                    <thead class="p-0 bg-da-yellow">
                                        <tr>
                                            <th>Planet</th>
                                            <th>RL-NL-SL-SSL</th>
                                            <th>Significators</th>
                                        </tr>
                                    </thead>

                                    <tbody>
                                        @for (int i = 0; i < 9; i++)
                                        {
                                            <tr>
                                                <td>सूर्य</td>
                                                <td>मंगल--सूर्य--चंद्र--केतु</td>
                                                <td>| 9 | 1 | 9 | 1</td>
                                            </tr>
                                        }

                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            @* KP *@
            <div class="tab-pane fade active show" id="custom-tabs-kp" role="tabpanel" aria-labelledby="custom-tabs-kp-tab">

                <div class="card-header p-0 mb-2">
                    <h3 class="font-weight">Bhav Chalit</h3>
                </div>
                @*  <div class="col">
                <button type="button" class="btn btn-outline-warning btn-sm" title="Bhav Chalit"> Bhav </button>
