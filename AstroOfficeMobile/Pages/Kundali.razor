            @* Date Finder *@
            <div class="tab-pane fade" id="custom-tabs-datefinder" role="tabpanel" aria-labelledby="custom-tabs-datefinder-tab">

                <div class="row mt-2">
                    <div class="col">
                        <div class="form-group">
                            <label>Select Country</label>
                            <select class="form-control form-control form-control-border">
                                <option>Child Birth - 2,5,11 - 18~55</option>
                                <option>Marriage - 2,7,11 - 18~55</option>
                                <option>Worst Days - 2,6,8,12 - 1~70</option>
                                <option>Child Worst Days - 1,4,6,8,12 - 18~80</option>
                                <option>Job Worst Days - 1,5,8,9,12 - 21~80</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="row mt-2">
                    <div class="col">
                        <div class="d-flex justify-content-between">
                            <div class="form-check">
                                <input type="checkbox" id="sahasane" class="form-check-input" />
                                <label class="form-check-label" for="sahasane">Sahasane</label>
                            </div>
                            <div class="form-check">
                                <input type="checkbox" id="fullMatch" class="form-check-input" />
                                <label class="form-check-label" for="fullMatch">Full match</label>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row mt-3">
                    <div class="col">
                        <h6 class="font-weight-bold">Choose Period</h6>
                        <div class="row">
                            <div class="col">
                                <div class="form-check">
                                    <input type="radio" name="DateFinderChoosePeriod" value="Period.Maha" class="form-check-input" id="rbMaha" />
                                    <label class="form-check-label" for="rbMaha">
                                        Maha
                                    </label>
                                </div>
                            </div>

                            <div class="col">
                                <div class="form-check">
                                    <input type="radio" name="DateFinderChoosePeriod" value="Period.Antar" class="form-check-input" id="rbAntar" />
                                    <label class="form-check-label" for="rbAntar">
                                        Antar
                                    </label>
                                </div>
                            </div>

                            <div class="col">
                                <div class="form-check">
                                    <input type="radio" name="DateFinderChoosePeriod" value="Period.Pryan" class="form-check-input" id="rbPryan" />
                                    <label class="form-check-label" for="rbPryan">
                                        Pryan
                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


                <div class="row mt-2">
                    <div class="col">
                        <div class="row">
                            <div class="col">
                                <div class="form-check">
                                    <input type="radio" name="DateFinderDashaRadioButtons" value="Dasha.Dasha" class="form-check-input" id="rbDasha" />
                                    <label class="form-check-label" for="rbDasha">
                                        Dasha
                                    </label>
                                </div>
                            </div>

                            <div class="col">
                                <div class="form-check">
                                    <input type="radio" name="DateFinderDashaRadioButtons" value="Dasha.NakSwami" class="form-check-input" id="rbNakswami" />
                                    <label class="form-check-label" for="rbNakswami">
                                        Nak Swami
                                    </label>
                                </div>
                            </div>

                            <div class="col">
                                <div class="form-check">
                                    <input type="radio" name="DateFinderDashaRadioButtons" value="Dasha.Both" class="form-check-input" id="rbForBoth" />
                                    <label class="form-check-label" for="rbForBoth">
                                        Both
                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row mt-2">
                    <div class="col">
                        <div class="d-flex ">
                            <button type="button" class="btn btn-primary">Show</button>
                        </div>
                    </div>
                </div>

                <div class="row mt-3">
                    <div class="date-finder col">
                        <form>
                            <textarea class="form-control" rows="13" placeholder="Date Lists ..." name="DateFinder.DateList"></textarea>
                        </form>
                    </div>
                </div>

            </div>

            @* Best Kundali *@
            <div class="tab-pane fade" id="custom-tabs-bestKundai" role="tabpanel" aria-labelledby="custom-tabs-bestKundai-tab">
                <div class="row">
                    <div class="col">
                        <div class="row align-items-end mt-2">
                            <div class="col-12">
                                <h6 class="font-weight-bold">Starting Date and Time</h6>
                            </div>
                            <div class="col">
                                <div>
                                    <input type="date" class="form-control" style="border: 1px solid #ccc; padding: 2px; height: 30px;" />
                                </div>
                            </div>
                            <div class="col">
                                <div>
                                    <input type="time" class="form-control" style="border: 1px solid #ccc; padding: 2px; height: 30px;" />
                                </div>
                            </div>

                        </div>
                    </div>
                </div>

                <div class="row mt-2">
                    <div class="col">
                        <div class="row align-items-end mt-2">
                            <div class="col-12">
                                <h6 class="font-weight-bold">Ending Date and Time</h6>
                            </div>
                            <div class="col">
                                <div>
                                    <input type="date" class="form-control" style="border: 1px solid #ccc; padding: 2px; height: 30px;" />
                                </div>
                            </div>
                            <div class="col">
                                <div>
                                    <input type="time" class="form-control" step="1" style="border: 1px solid #ccc; padding: 2px; height: 30px;" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


                <div class="row mt-3">
                    <div class="col">
                        <div class="d-flex justify-content-between align-items-center">
                            <label class="text-nowrap mr-3">Choose Rating</label>
                            <select class="form-control custom-select" aria-label="Selection of rating">
                                <!-- You can use your data source to generate the options -->
                                <option value="Option1">Good</option>
                                <option value="Option2">Best</option>
                                <option value="Option3">Excellent</option>
                                <!-- Repeat this for other options -->
                            </select>
                        </div>
                    </div>
                </div>

                <div class="row mt-3">
                    <div class="col">
                        <div class="form-check">
                            <input type="radio" name="DateFinderDashaRadioButtons" value="Dasha.Dasha" class="form-check-input" id="rbKp" />
                            <label class="form-check-label" for="rbKp">
                                KP
                            </label>
                        </div>
                    </div>



                    <div class="col">
                        <div class="form-check">
                            <input type="radio" name="DateFinderDashaRadioButtons" value="Dasha.Both" class="form-check-input" id="rbRedBook" />
                            <label class="form-check-label" for="rbRedBook">
                                Red Book
                            </label>
                        </div>
                    </div>
                </div>

                <div class="row align-items-center">

                    <div class="col">
                        <label class="mb-0">
                            countProcess
                        </label>
                    </div>

                    <div class="col">

                        <div class="d-flex flex-row-reverse">
                            <button type="button" class="btn btn-primary">Show</button>
                        </div>
                    </div>
                </div>



                <div class="row mt-3">
                    <div class="date-finder col">
                        <form>
                            <textarea class="form-control" rows="13" placeholder="Best Dates ..." name="DateFinder.DateList"></textarea>
                        </form>
                    </div>
                </div>


            </div>
        </div>
    </div>
    <!-- /.card -->
</div>
