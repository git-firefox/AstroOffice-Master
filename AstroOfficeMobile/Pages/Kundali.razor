                *@
                <div class="mt-2">
                    <div class="overlay"></div>
                    <div>
                        <img class="img-fluid text-center" src="data:image/png;base64," />
                    </div>

                </div>
                <div>
                    <div class="card-header p-0 mb-2 mt-2">
                        <h3 class="font-weight">Kundali Table</h3>
                    </div>
                    <div class="card-body p-0">
                        <div class="table-container">
                            <table class="table table-sm table table-bordered">
                                <thead class="p-0 bg-da-yellow">
                                    <tr>
                                        <th>Desk</th>
                                        <th>Value</th>
                                    </tr>
                                </thead>
                                <tbody class="data-loader">
                                    @for (int i = 0; i < 12; i++)
                                    {
                                        <tr>
                                            <td>महादशा</td>
                                            <td>शनि 11 5 6 11 5 6</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>




            </div>
            @* Mahadasha *@
            <div class="tab-pane fade" id="custom-tabs-mahadasha" role="tabpanel" aria-labelledby="custom-tabs-mahadasha-tab">

