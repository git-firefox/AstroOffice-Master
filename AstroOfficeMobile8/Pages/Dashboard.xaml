<?xml version="1.0" encoding="utf-8" ?>
<Shell
    x:Class="AstroOfficeMobile8.Pages.Dashboard"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:account="clr-namespace:AstroOfficeMobile8.Account"
    xmlns:local="clr-namespace:AstroOfficeMobile8.Pages"
    Title="Dashboard"
    FlyoutBackgroundColor="#eeeeee"
    Shell.FlyoutBehavior="Flyout">
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:Home}"
        Route="Home" />

    <ShellContent
        Title="Shop"
        ContentTemplate="{DataTemplate local:Shop}"
        Route="Shop" />

    <ShellContent
        Title="Account"
        ContentTemplate="{DataTemplate account:LoginPage}"
        Route="login" />

    <ShellContent
        Title="Saved Kundali"
        ContentTemplate="{DataTemplate local:SavedKundali}"
        Route="savedKundali" />

    <ShellContent
        Title="Kundali"
        ContentTemplate="{DataTemplate local:Kundali}"
        Route="Kundali" />

    <ShellContent
        Title="Basic Details"
        ContentTemplate="{DataTemplate local:BasicDetails}"
        Route="Basicdetails" />

</Shell>

