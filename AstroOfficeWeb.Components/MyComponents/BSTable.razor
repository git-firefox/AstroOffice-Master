@* @typeparam TItem *@
<table class="table">
    <thead>
        <tr>
            @foreach (var item in THeadItems)
            {
                <th scope="col">@item</th>
            }
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < 10; i++)
        {
            <tr>
                <td>
                    @TBodyTemplate(item)
                </td>
            </tr>
        }
    </tbody>
</table>