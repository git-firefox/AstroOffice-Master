@page "/dashboard"

@attribute [Authorize(Roles = "Admin,User")]

@inherits BaseComponent

@inject HttpClient HttpClient
@inject IJSRuntime JSRuntime
@inject ISnackbar Snackbar

@inject BestBLLService BestBLL
@inject KPBLLService KPBLL
@inject KPDAOService KPDAO
@inject KundliBLLService KundliBLL
@inject PredictionBLLService PredictionBLL
@inject KundaliHistroyService KundaliHistroy
@inject ISwaggerApiService Swagger
@inject TokenWalletService TokenWallet


<div class="container-fluid">
    <h2 class="mt-2">Report</h2>

    <div class="row">

        @* Birth Details *@
        <div class="col-xl-12">
            <div class="card border-img bg-custom">
                <div class="card-header bg-transparent text-md-center border-bottom-2 d-flex justify-content-between">
                    <h2 class="fw-bold">Birth Details</h2>
                    <button class="btn btn-sm btn-custom " type="button" data-bs-toggle="tooltip" data-bs-placement="top" data-bs-title="Refresh Kundali">Refresh</button>
                </div>
                <div class="card-body">
                    <EditForm Model="BirthDetails">
                        <div class="row row-cols-sm-3 gy-2">
                            <!-- TxtName InputText -->
                            <div class="col-12 col-md-4">
                                <input type="text" class="form-control form-control-sm" placeholder="Name" title="Name" />
                            </div>

                            <!-- Gender InputRadioGroup -->
                            <div class="col-6 col-md-4">
                                <div class="form-check">
                                    <input type="radio" class="form-check-input" id="RbMale" />
                                    <label class="form-check-label" for="RbMale">Male</label>
                                </div>
                            </div>

                            <div class="col-6 col-md-4">
                                <div class="form-check">
                                    <input type="radio" class="form-check-input" id="RbFemale" />
                                    <label class="form-check-label" for="RbFemale">Female</label>
                                </div>
                            </div>

                            <!-- Date of Birth Input -->
                            <div class="col-12 col-md-3">
                                <input type="date" class="form-control form-control-sm" title="Date of Birth" max="9999-12-31" />
                            </div>

                            <!-- Hour Select -->
                            <div class="col-4 col-md-3">
                                <select class="form-select form-select-sm" title="Hour">
                                    <!-- Replace with actual options -->
                                    <option value="1">01</option>
                                    <option value="2">02</option>
                                </select>
                            </div>
                            <!-- Minute Select -->
                            <div class="col-4 col-md-3">
                                <select class="form-select form-select-sm" title="Minute">
                                    <!-- Replace with actual options -->
                                    <option value="0">00</option>
                                    <option value="15">15</option>
                                </select>
                            </div>
                            <!-- Second Select -->
                            <div class="col-4 col-md-3">
                                <select class="form-select form-select-sm" title="Second">
                                    <!-- Replace with actual options -->
                                    <option value="0">00</option>
                                    <option value="30">30</option>
                                </select>
                            </div>

                            <!-- SkipBad Select -->
                            <div class="col-6 col-md-3">
                                <select class="form-select form-select-sm" aria-label="Select SkipBad " title="Skip Bad" @onchange="OnChange_CmbSkipBad" value="@BirthDetails!.CmbSkipBad">
                                    @foreach (var item in BirthDetailsLookups.SkipBadItems)
                                    {
                                        <option value="@item.Text">@item.Text</option>
                                    }
                                </select>
                            </div>

                            <!-- CmbTime Select -->
                            <div class="col-6 col-md-3">
                                <select class="form-select form-select-sm" aria-label="Select Time" title="Time" @onchange="OnChange_CmbTime" value="@BirthDetails!.CmbTime">
                                    @foreach (var item in BirthDetailsLookups.CmbTimeItems)
                                    {
                                        <option value="@item.Text.ToLower()">@item.Text</option>
                                    }
                                </select>
                            </div>
                        </div>

                        <div class="row mt-2">

                            <!-- Minus Button -->
                            <div class="col-auto">
                                <button type="button" class="btn btn-sm btn-custom " title="Subtract from Brith Date or Time" @onclick="OnClick_BtnMinus"> <i class="fa fa-minus"></i> </button>
                            </div>

                            <!-- TimeValue InputNumber -->
                            <div class="col">
                                <input type="number" class="form-control form-control-sm" title="Enter number to modify date" />
                            </div>

                            <!-- Plus Button -->
                            <div class="col-auto">
                                <button type="button" class="btn btn-sm btn-custom " title="Add to Birth Date or Time"> <i class="fa fa-plus"></i> </button>
                            </div>
                        </div>

                        <div class="row gy-2 mt-2">
                            <div class="col-12 col-md-6">

                                <!-- CmbCountry Select -->
                                <select class="form-select form-select-sm" aria-label="Select Brith Country">
                                    <!-- Replace with actual options -->
                                    <option value="country1">Country 1</option>
                                    <option value="country2">Country 2</option>
                                </select>
                            </div>
                            <div class="col-12 col-md-6">
                                <!-- TxtBirthPlace Input -->
                                <input type="search" class="form-control form-control-sm" placeholder="Search Birth Place" title="Birth Place" />

                            </div>
                            <div class="col-12">

                                <!-- ListBirthCities Select -->
                                <select class="form-select form-select-sm " aria-label="Select BirthCity" size="3">
                                    <option value="0">-- Select --</option>
                                    <!-- Replace with actual options -->
                                    <option value="city1">City 1</option>
                                    <option value="city2">City 2</option>
                                </select>
                            </div>

                            <div class="col-6 col-md-4">


                                <!-- Longtitude InputText -->
                                <div class="form-floating">
                                    <input id="InputLongtitude" class="form-control form-control-sm" type="text" title="Longtitude" placeholder="Longtitude" />
                                    <label for="InputLongtitude" class="me-2"> Longtitude </label>
                                </div>
                            </div>

                            <div class="col-6 col-md-4">
                                <!-- Latitude InputText -->
                                <div class="form-floating">
                                    <input id="InputLongtitude" class="form-control form-control-sm" type="text" title="Latitude" placeholder="Latitude" />
                                    <label for="Latitude" class="me-2"> Latitude </label>
                                </div>

                            </div>
                            <div class="col-12 col-md-4">
                                <!-- TxtTimezone Input -->
                                <div class="form-floating">
                                    <input id="InputLongtitude" class="form-control form-control-sm" type="text" title="Timezone" placeholder="Timezone" />
                                    <label for="Timezone" class="me-2"> Timezone </label>
                                </div>
                            </div>

                        </div>

                        <div class="row row-cols-sm-1 gy-2 mt-2">
                            <div class="col-12 col-md-4">

                                <!-- CmbCategory Select -->
                                <select class="form-select form-select-sm" title="Category">
                                    <!-- Replace with actual options -->
                                    <option value="category1">Category 1</option>
                                    <option value="category2">Category 2</option>
                                </select>
                            </div>

                            <div class="col-12 col-md-4">
                                <!-- CmbAyanansh Select -->
                                <select class="form-select form-select-sm" title="Ayanansh">
                                    <!-- Replace with actual options -->
                                    <option value="ayanansh1">Ayanansh 1</option>
                                    <option value="ayanansh2">Ayanansh 2</option>
                                </select>
                            </div>

                            <div class="col-12 col-md-4">
                                <!-- CmbLanguage Select -->
                                <select class="form-select form-select-sm" title="Language">
                                    <!-- Replace with actual options -->
                                    <option value="language1">Language 1</option>
                                    <option value="language2">Language 2</option>
                                </select>
                            </div>
                        </div>


                        <div class="row row-cols-sm-2 gy-2 mt-2">

                            <div class="col-6 col-md-3">
                                <div class="form-check">
                                    <input type="checkbox" id="ChkSahasane" class="form-check-input" @bind="BirthDetails!.ChkSahasaneLogic" />
                                    <label class="form-check-label" for="ChkSahasane">Sahasane Logic</label>
                                </div>
                            </div>

                            <div class="col-6 col-md-3">
                                <div class="form-check">
                                    <input type="checkbox" id="ChkSalachakkar" class="form-check-input" @bind="BirthDetails!.SalaChakkar" />
                                    <label class="form-check-label" for="ChkSalachakkar">35 Sala Chakkar</label>

                                </div>
                            </div>

                            <div class="col-6 col-md-3">
                                <div class="form-check">
                                    <input id="ChkMfal" class="form-check-input" type="checkbox" @bind="BirthDetails!.ChkMfal" />
                                    <label class="form-check-label" for="ChkMfal">Mfal</label>

                                </div>
                            </div>

                            <div class="col-6 col-md-3">
                                <div class="form-check">
                                    <input id="ref" class="form-check-input" type="checkbox" />
                                    <label class="form-check-label" for="ref">Show Ref. Code</label>
                                </div>
                            </div>
                        </div>

                    </EditForm>
                </div>

                <div class="card-footer bg-transparent border-top-2">

                    <div class="d-grid gap-2 d-flex justify-content-between justify-content-md-end ">
                        <button class="btn btn-custom btn-sm" type="button">Show Kundali</button>
                        <button class="btn btn-custom btn-sm" type="button">Vivran</button>
                        <button class="btn btn-custom btn-sm me-sm-2" type="button"> Save Kundali </button>
                        <!-- <button class="btn btn-custom btn-sm" type="button" disabled>
                                <span class="spinner-border spinner-border-sm" aria-hidden="true"></span>
                                <span role="status">Loading...</span>
                            </button> -->
                    </div>
                </div>
            </div>
        </div>

        <div class="col-lg-6 col-xl-6 my-3 my-lg-5 astro-report-kundali-table">
            <div class="card border-img bg-custom">
                <div class="card-header bg-transparent border-bottom-2 text-center">
                    <h2 class="fw-bold">Kundali Table</h2>
                </div>
                <div class="card-body">
                    <div class="row">
                        <div class="col-md-12">
                            <table class="table table-sm table-hover">
                                <thead>
                                    <tr>
                                        <th>Desk</th>
                                        <th>Value</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>महादशा</td>
                                        <td>शुक्र 8 9 2 8 9 2</td>
                                    </tr>
                                    <tr>
                                        <td>अन्तर्दशा</td>
                                        <td>शनि 10 5 6 10 5 6</td>
                                    </tr>
                                    <tr>
                                        <td>प्रयंतरदशा</td>
                                        <td>शनि 10 5 6 10 5 6</td>
                                    </tr>
                                    <tr>
                                        <td>सुक्ष्मदशा</td>
                                        <td>शुक्र 8 9 2 8 9 2</td>
                                    </tr>
                                    <tr>
                                        <td>लाल दशा</td>
                                        <td>राहु</td>
                                    </tr>
                                    <tr>
                                        <td>भाग्यशाली अंक</td>
                                        <td>8 3</td>
                                    </tr>
                                    <tr>
                                        <td>भाग्यशाली दिन</td>
                                        <td>शनिवार</td>
                                    </tr>
                                    <tr>
                                        <td>जनमदिन</td>
                                        <td>सोमवार</td>
                                    </tr>
                                    <tr>
                                        <td>जनमदिन ग्रह</td>
                                        <td>चंद्र</td>
                                    </tr>
                                    <tr>
                                        <td>जनमसमय ग्रह</td>
                                        <td>शुक्र</td>
                                    </tr>
                                    <tr>
                                        <td>राशि</td>
                                        <td>धनु</td>
                                    </tr>
                                    <tr>
                                        <td>नक्षत्र</td>
                                        <td>मूल 2</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-lg-6 col-xl-6 my-lg-5 astro-report-kundali-chart">
            <div class="card border-img bg-custom">
                <div class="card-header bg-transparent border-bottom-2 text-center">
                    <h2 class="fw-bold">Kundali</h2>
                </div>

                <div class="card-body">
                    <form>
                        <div class="row gy-5">
                            <div class="col-9">
                                <div class="overflow-x-scroll">
                                    <div class="d-grid gap-2 d-flex justify-content-between">
                                        <button type="button" class="btn btn-custom btn-sm" title="Cusp"> Cusp </button>
                                        <button type="button" class="btn btn-custom btn-sm" title="Kundali"> Kundali </button>
                                        <button type="button" class="btn btn-custom btn-sm" title="Chandra Kundali"> Chandra </button>
                                        <button type="button" class="btn btn-custom btn-sm" title="Vartmaan"> Vartmaan </button>
                                        <button type="button" class="btn btn-custom btn-sm" title="Varshphal"> Varshphal </button>
                                    </div>
                                </div>
                            </div>

                            <div class="col-3">
                                <input step="any" min="1" max="120" type="number" class="form-control form-control-sm " />
                            </div>

                            <div class="col-12">
                                <div class="bg-astro">
                                    <img src="data:image/png;base64," alt="" class="img-fluid" />
                                </div>
                            </div>
                        </div>


                        <div class="row mt-2 text-center">
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>

    <div class="row astro-report">
        <div class="col">
            <BSTabs BSTabName="Report Tabs">
                <BSTabItems>
                    <BSTabItem TabName="Charts" IsActive="true" />
                    <BSTabItem TabName="Mahadasha" />
                    <BSTabItem TabName="Date Finder" />
                    <BSTabItem TabName="Best Kundali Dates" />
                </BSTabItems>
                <BSTabContent>
                    <BSTabPanel TabPanelName="Charts" IsActive="true">
                        <TabPanelBody>
                            <p1>Charts</p1>
                        </TabPanelBody>
                    </BSTabPanel>
                    <BSTabPanel TabPanelName="Mahadasha">
                        <TabPanelBody>
                            <p1>Mahadasha</p1>
                        </TabPanelBody>
                    </BSTabPanel>
                    <BSTabPanel TabPanelName="Date Finder">
                        <TabPanelBody>
                            <p1>Date Finder</p1>
                        </TabPanelBody>
                    </BSTabPanel>
                    <BSTabPanel TabPanelName="Best Kundali Dates">
                        <TabPanelBody>
                            <p1>Best Kundali Dates</p1>
                        </TabPanelBody>
                    </BSTabPanel>
                </BSTabContent>
            </BSTabs>

        </div>
    </div>
</div>

