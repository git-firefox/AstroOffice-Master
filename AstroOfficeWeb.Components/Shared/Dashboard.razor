@page "/dashboard"

@attribute [Authorize(Roles = "Admin,User")]

@inherits BaseComponent

@inject HttpClient HttpClient
@inject IJSRuntime JSRuntime
@inject ISnackbar Snackbar

@inject BestBLLService BestBLL
@inject KPBLLService KPBLL
@inject KPDAOService KPDAO
@inject KundliBLLService KundliBLL
@inject PredictionBLLService PredictionBLL
@inject KundaliHistroyService KundaliHistroy
@inject ISwaggerApiService Swagger
@inject TokenWalletService TokenWallet


<FaladeshModal @ref="FaladeshModal" Falla="@htmlStringFalla" ImgSrcLagan="@imgSrcLagan" ImgSrcBhavChalit="@imgSrcBhavChalit" ConfirmationChanged="ConfirmationChangedModel" />

@* @User?.Identity?.Name *@
<div class="container-fluid p-3">
    <h1 class="py-4 font-weight-bold text-center text-md-left">Report</h1>
    <section class="content">
        <div class="row report-content mb-3">

            @* Birth Details *@
            <div class="col-xl-4 mb-lg-3 mb-xl-0">
                <div class="card card-primary card-frame">
                    <div style="position:relative;z-index:999;padding:15px;">
                        <div class="card-header p-0 text-center mb-2">
                            <h2 class="font-weight-bold">Birth Details</h2>
                        </div>
                        <EditForm Model="BirthDetails">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col">
                                        <select @onchange="OnChange_CmbSkipBad" value="@BirthDetails!.CmbSkipBad" class="form-control form-control-sm" aria-label="Selection of SkipBad" title="Skip Bad">
                                            @foreach (var item in BirthDetailsLookups.SkipBadItems)
                                            {
                                                <option value="@item.Text">@item.Text</option>
                                            }
                                        </select>
                                    </div>
                                    <div class="col">
                                        <select @onchange="OnChange_CmbTime" value="@BirthDetails!.CmbTime" class="form-control form-control-sm" aria-label="Selection of Time" title="Time">
                                            @foreach (var item in BirthDetailsLookups.CmbTimeItems)
                                            {
                                                <option value="@item.Text.ToLower()">@item.Text</option>
                                            }
                                        </select>
                                    </div>
                                    <div class="col-auto">
                                        <button type="button" class="btn btn-block btn-custom btn-sm" @onclick="OnClick_BtnMinus">  <i class="fa fa-minus"></i> </button>
                                    </div>
                                    <div class="col-2">
                                        <InputNumber @bind-Value="BirthDetails.TimeValue" class="form-control form-control-sm" title="Number to modifying date" />
                                    </div>
                                    <div class="col-auto">
                                        <button type="button" class="btn btn-block btn-custom btn-sm" @onclick="OnClick_BtnPlus"> <i class="fa fa-plus"></i> </button>
                                    </div>
                                    <div class="col-auto">
                                        <button type="button" class="btn btn-block btn-custom btn-sm" @onclick="OnClick_BtnRefresh">Refresh</button>
                                    </div>
                                </div>

                                <div class="row mt-2">
                                    <div class="col-6">
                                        <div class="row">
                                            <div class="col-12">
                                                @if (IsAdmin || UserCanEdit)
                                                {
                                                    <InputText @ref="inputTextName" @bind-Value="BirthDetails!.TxtName" class="form-control form-control-sm mb-2" />
                                                }
                                                else
                                                {
                                                    <input value="@BirthDetails!.TxtName" class="form-control form-control-sm mb-2" readonly />
                                                }

                                                @if (CmbCountry != null)
                                                {
                                                    <select @bind="BirthDetails!.CmbCountry" class="form-control form-control-sm mb-2" aria-label="Selection of brith country">
                                                        @foreach (var item in CmbCountry)
                                                        {
                                                            <option value="@item.Value">@item.Text</option>
                                                        }
                                                    </select>
                                                }
                                                @if ((IsAdmin || UserCanEdit) && inputBirthPlaceEnabled)
                                                {
                                                    @* <InputText @ref="inputTextBirthPlace" @bind-Value="BirthDetails!.TxtBirthPlace" class="form-control form-control-sm mb-2" @onkeydown="OnKeyDown_TxtBirthplace" @oninput="OnInput_TxtBirthplace" @onfocus="OnFocus_TxtBirthplace" /> *@
                                                    <input class="form-control form-control-sm mb-2" @bind="BirthDetails!.TxtBirthPlace" @bind:event="oninput" @onkeyup="OnKeyDown_TxtBirthplace" />
                                                }
                                                else
                                                {
                                                    <input value="@BirthDetails!.TxtBirthPlace" class="form-control form-control-sm mb-2" readonly />
                                                }
                                                @if (ListBirthCities != null)
                                                {

                                                    <select value="@selectedBirthCityIndex" class="form-control form-control-sm mb-2" aria-label="Selection of BirthCity" size="3" @onchange="OnChange_ListBirthCities">
                                                        <option value="0">-- Select --</option>

                                                        @*     ListBirthCities.Insert(0, "Select"); *@

                                                        @for (int i = 0; i < ListBirthCities.Count; i++)
                                                        {

                                                            @* <option value="@i" @onclick="e => OnClick_ListBirthCity(e, i)">@ListBirthCities[i].Place</option> *@
                                                            <option value="@(i + 1)">@ListBirthCities[i].Place</option>
                                                        }
                                                    </select>
                                                }

                                                <div class="row">
                                                    <div class="col-6">
                                                        <div class="form-group row align-items-center mb-2">
                                                            <label for="IdLongtitude" class="col-12">Long</label>
                                                            <div class="col-12">
                                                                <InputText @bind-Value="BirthDetails!.Longtitude" id="IdLongtitude" class="form-control form-control-sm" title="Longtitude" />
                                                            </div>
                                                        </div>
                                                        <select value="@BirthDetails!.CmbLanguage" class="form-control form-control-sm mb-2" aria-label="Selection of language" @onchange="OnChange_CmbLanguage" title="Language">
                                                            @foreach (var item in BirthDetailsLookups.LanguageItems)
                                                            {
                                                                <option value="@item.Text">@item.Text</option>
                                                            }
                                                        </select>
                                                    </div>
                                                    <div class="col-6">
                                                        <div class="form-group row align-items-center mb-2">
                                                            <label for="IdLatitude" class="col-12">Lat</label>
                                                            <div class="col-12">

                                                                <InputText @bind-Value="BirthDetails!.Latitude" id="IdLatitude" class="form-control form-control-sm" title="Latitude" />
                                                            </div>
                                                        </div>
                                                        @if (IsAdmin || UserCanEdit)
                                                        {
                                                            <InputText @bind-Value="BirthDetails!.TxtTimezone" class="form-control form-control-sm mb-2" title="Timezone" />
                                                        }
                                                        else
                                                        {
                                                            <input value="@BirthDetails!.TxtTimezone" class="form-control form-control-sm mb-2" title="Timezone" readonly />
                                                        }
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-6">
                                        <div class="row">
                                            <div class="col-12 mb-2">
                                                @if (inputBirthDateEnabled)
                                                {
                                                    @* <input type="date" class="form-control form-control-sm" @onchange="OnChange_BrithDate" value="1999-09-20" title="Date of Birth" max="9999-12-31" /> *@
                                                    <input type="date" class="form-control form-control-sm" @onchange="OnChange_BrithDate" value="@BirthDetails.DateOfBirth.ToString(format: "yyyy-MM-dd")" title="Date of Birth" max="9999-12-31" />
                                                }
                                                else
                                                {
                                                    <input type="date" class="form-control form-control-sm" value="@BirthDetails.DateOfBirth.ToString(format: "yyyy-MM-dd")" title="Date of Birth" max="9999-12-31" disabled />
                                                }
                                            </div>

                                            <div class="col-4 mb-2 d-flex">
                                                @if (tobhhEnabled)
                                                {
                                                    @* <input @bind="inputHour" class="form-control form-control-sm" min="1" max="24" @onfocusout="OnFocusOut_InputBrithDate" title="Hour" /> *@

                                                    <select value="@BirthDetails!.Tobhh" class="form-control form-control-sm mb-2" title="Hour" @onchange="OnChange_Hour">
                                                        @for (int hour = 0; hour <= 23; hour++)
                                                        {
                                                            <option value="@hour">@hour.ToString("D2")</option>
                                                        }
                                                    </select>
                                                    <span> : </span>
                                                }
                                                else
                                                {
                                                    <input value="@BirthDetails!.Tobhh.ToString("D2")" class="form-control form-control-sm" title="Hour" readonly />
                                                }
                                            </div>
                                            <div class="col-4 mb-2 d-flex">
                                                @if (tobmmEnabled)
                                                {
                                                    @* <input @bind="inputMinute" class="form-control form-control-sm" min="1" max="60" @onfocusout="OnFocusOut_InputBrithDate" title="Minute" /> *@

                                                    <select value="@BirthDetails!.Tobmm" class="form-control form-control-sm mb-2" title="Minute" @onchange="OnChange_Minute">
                                                        @for (int minute = 0; minute <= 59; minute++)
                                                        {
                                                            <option value="@minute">@minute.ToString("D2")</option>
                                                        }
                                                    </select>
                                                    <span> : </span>
                                                }
                                                else
                                                {
                                                    <input value="@BirthDetails!.Tobmm.ToString("D2")" class="form-control form-control-sm" title="Minute" readonly />
                                                }
                                            </div>
                                            <div class="col-4 mb-2 ">
                                                @if (tobssEnabled)
                                                {
                                                    @* <input @bind="inputSecond" class="form-control form-control-sm" min="0" max="60" @onfocusout="OnFocusOut_InputBrithDate" title="Second" /> *@

                                                    <select value="@BirthDetails!.Tobss" class="form-control form-control-sm mb-2" title="Second" @onchange="OnChange_Second">
                                                        @for (int second = 0; second <= 59; second++)
                                                        {
                                                            <option value="@second">@second.ToString("D2")</option>
                                                        }
                                                    </select>

                                                }
                                                else
                                                {
                                                    <input value="@BirthDetails!.Tobss.ToString("D2")" class="form-control form-control-sm" title="Second" readonly />
                                                }
                                            </div>

                                            <div class="col-12">

                                                <fieldset>
                                                    @* <legend class="w-auto small mb-0">Options</legend> *@
                                                    <div class="row mb-2">
                                                        <InputRadioGroup Name="Gender" @bind-Value="BirthDetails!.Gender">

                                                            <div class="col">
                                                                <div class="form-check">
                                                                    <InputRadio Value="Gender.Male" class="form-check-input" id="RbMale" />
                                                                    <label class="form-check-label" for="RbMale">
                                                                        Male
                                                                    </label>
                                                                </div>
                                                            </div>

                                                            <div class="col">
                                                                <div class="form-check">
                                                                    <InputRadio Value="Gender.Female" class="form-check-input" id="RbFemale" />
                                                                    <label class="form-check-label" for="RbFemale">
                                                                        Female
                                                                    </label>
                                                                </div>
                                                            </div>

                                                            @*  <div class="col-3">
                                                            <button type="button" class="btn btn-custom"> ✓ </button>
                                                            </div> *@


                                                        </InputRadioGroup>
                                                    </div>


                                                    <div class="row">
                                                        <div class="col-12 mb-2">

                                                            <select value="@BirthDetails!.CmbCategory" @onchange="OnChange_CmbCategory" class="form-control form-control-sm" title="Category">
                                                                @foreach (var item in BirthDetailsLookups.BirthCategoryItems.Where(a => !a.Disabled))
                                                                {
                                                                    <option value="@item.Value">@item.Text</option>
                                                                }
                                                            </select>

                                                        </div>
                                                        <div class="col-6 mb-2">
                                                            @* <InputSelect @bind-Value="BirthDetails!.CmbAyanansh" class="form-control form-control-sm" aria-label="Default select example"> *@
                                                            <select value="@BirthDetails.CmbAyanansh" @onchange="OnChange_CmbAyanansh" class="form-control form-control-sm" aria-label="Selection of ayanansh" title="Ayanansh">
                                                                @foreach (var item in BirthDetailsLookups.CmbAyananshItems)
                                                                {
                                                                    <option value="@item.Text">@item.Text</option>
                                                                }
                                                            </select>
                                                        </div>
                                                        <div class="col-6 mb-2">
                                                            <select value="@BirthDetails.CmbRotate" @onchange="OnChange_CmbRotate" class="form-control form-control-sm" title="Rotate">
                                                                @foreach (var item in BirthDetailsLookups.CmbRotateItems)
                                                                {
                                                                    <option value="@item.Text">@item.Text</option>
                                                                }
                                                            </select>
                                                        </div>
                                                        <div class="col-12 mb-2">
                                                            <div class="form-check">
                                                                <InputCheckbox id="ChkSahasane" class="form-check-input" @bind-Value="BirthDetails!.ChkSahasaneLogic" />
                                                                <label class="form-check-label" for="ChkSahasane">Sahasane Logic</label>
                                                            </div>
                                                        </div>
                                                        <div class="col-12 mb-2">
                                                            <div class="form-check">
                                                                <InputCheckbox id="ChkSalachakkar" class="form-check-input" @bind-Value="BirthDetails!.SalaChakkar" />
                                                                <label class="form-check-label" for="ChkSalachakkar">35 Sala Chakkar</label>
                                                            </div>
                                                        </div>

                                                        <div class="col-12 mb-2">
                                                            <div class="form-check">
                                                                <InputCheckbox id="ChkMfal" class="form-check-input" @bind-Value="BirthDetails!.ChkMfal" />
                                                                <label class="form-check-label" for="ChkMfal">Mfal</label>
                                                            </div>
                                                        </div>
                                                        @*  <div class="col">
                                                        <InputTextArea class="form-control mt-2" rows="4" Placeholder="Enter ..." @bind-Value="BirthDetails!.Brief" />
                                                        </div> *@
                                                    </div>
                                                </fieldset>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <div class="d-flex bd-highlight">
                                    <div class="p-2 flex-grow-1 bd-highlight">
                                        <div class="form-check form-check-inline">
                                            <InputCheckbox id="ref" class="form-check-input" @bind-Value="BirthDetails!.ChkShowRef" />
                                            <label class="form-check-label" for="ref">Show Ref. Code</label>
                                        </div>
                                    </div>
                                    <div class="p-2 bd-highlight">
                                        <button class="btn btn-block btn-custom btn-sm" @onclick="OnClick_BtnFaladesh"> Show Kundali </button>
                                    </div>
                                    <div class="p-2 bd-highlight">
                                        <button class="btn btn-block btn-custom btn-sm" @onclick="OnClick_BtnChart"> Vivran </button>
                                    </div>
                                    <div class="p-2 bd-highlight">
                                        @if (OnInitialized_IsComplatedSuccessfully)
                                        {
                                            <button class="btn btn-block btn-custom btn-sm" @onclick="OnClick_BtnSaveKundali"> Save Kundali </button>
                                        }
                                        else
                                        {
                                            <button class="btn btn-custom btn-sm" type="button" disabled>
                                                <span class="spinner-border spinner-border-sm" role="status" aria-hidden="true"></span>
                                                Loading...
                                            </button>
                                        }
                                    </div>
                                </div>
                            </div>
                        </EditForm>
                    </div>
                </div>
            </div>

            @* Kundali Table *@
            <div class="col-lg-6 col-xl-4 my-3 my-lg-0">
                <div class="card card-success card-frame">

                    <div style="position:relative;z-index:999;padding:15px;">
                        <div class="card-header p-0 text-center mb-2">
                            <h2 class="font-weight-bold">Kundali Table</h2>
                        </div>
                        <div class="card-body">
                            <div class="row">
                                <div class="col-md-12">
                                    <table class="table table-sm table-hover">

                                        <thead class="p-0">
                                            <tr>
                                                <th>Desk</th>
                                                <th>Value</th>
                                            </tr>
                                        </thead>

                                        @if (ListView_Ruling_Planet == null)
                                        {
                                            <tbody class="data-loader">
                                                <tr>
                                                    <td><div class="overlay"><i class="fas fa-2x fa-sync fa-spin"></i></div></td>
                                                </tr>
                                                @for (int i = 0; i < 12; i++)
                                                {
                                                    <tr>
                                                        <td>महादशा</td>
                                                        <td>गुरू 7 3 5 8</td>
                                                    </tr>
                                                }

                                            </tbody>

                                        }
                                        else
                                        {
                                            <tbody>
                                                @foreach (var row in ListView_Ruling_Planet ?? new List<KundaliTableTRModel>())
                                                {
                                                    <tr class="@row.SelectedClass">
                                                        <td>@row.Item</td>
                                                        <td>@row.SubItem</td>
                                                    </tr>
                                                }
                                            </tbody>
                                        }

                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            @* Kundali *@
            <div class="col-lg-6 col-xl-4">
                <div class="card card-primary card-frame">

                    <div style="position:relative;z-index:999;padding:15px;">
                        <div class="card-header p-0 text-center mb-2">
                            <h2 class="font-weight-bold">Kundali</h2>
                        </div>

                        <EditForm Model="BirthDetails">
                            <div class="card-body">

                                <div class="row">
                                    <div class="col">
                                        <button type="button" class="btn btn-block btn-custom btn-sm" title="Cusp" @onclick="OnClick_BhavChalit"> Cusp </button>
                                    </div>
                                    <div class="col">
                                        <button type="button" class="btn btn-block btn-custom btn-sm" title="Kundali" @onclick="OnClick_Lagan"> Kundali </button>
                                    </div>
                                    <div class="col">
                                        <button type="button" class="btn btn-block btn-custom btn-sm" title="Chandra Kundali" @onclick="OnClick_Chandra"> Chandra </button>
                                    </div>
                                    <div class="col">
                                        <button type="button" class="btn btn-block btn-custom btn-sm btn-gochar" title="Vartmaan" @onclick="OnClick_Gochar"> Vartmaan </button>
                                    </div>
                                    <div class="col">
                                        <button type="button" class="btn btn-block btn-custom btn-smbtn btn-block btn-custom btn-sm btn-varsh" title="Varshphal" @onclick="OnClick_Varsh"> Varshphal </button>
                                    </div>
                                </div>

                                <div class="row justify-content-end mt-2">
                                    <div class="col-2">
                                        @if (isNumVarshVisible)
                                        {
                                            <InputNumber id="1" @bind-Value="BirthDetails.KundaliUdvYear" class="form-control form-control-sm" min="1" max="120" />
                                        }
                                        else
                                        {

                                            <InputNumber @bind-Value="BirthDetails.KundaliUdvYear" class="form-control form-control-sm" style="opacity:0" min="1" max="120" />
                                        }
                                    </div>
                                </div>



                                @if (!string.IsNullOrEmpty(imgSrc))
                                {
                                    <div class="row mt-2">
                                        <div class="col-12 text-center">
                                            <img src="@imgSrc" alt="" class="img-fluid text-center" />
                                        </div>
                                    </div>
                                }
                                else
                                {
                                    <div class="data-loader mt-2">
                                        <div class="overlay"><i class="fas fa-2x fa-sync fa-spin"></i></div>
                                        <div>
                                            <img class="img-fluid text-center" src="data:image/png;base64," />
                                        </div>

                                    </div>
                                }

                            </div>
                        </EditForm>
                    </div>
                </div>
            </div>
        </div>

        <div class="row mt-5 pt-2">
            <div class="col">
                <div class="card card-tabs">
                    <div class="card-header pb-0">
                        <ul class="nav nav-tabs" id="birthdetails-tabs" role="tablist">
                            <li class="nav-item">
                                <a class="nav-link active" id="birthdetails-tabs-charts-tab" data-bs-toggle="pill" data-toggle="pill" href="#birthdetails-tabs-charts" role="tab" aria-controls="birthdetails-tabs-charts" aria-selected="true"> Charts </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" id="birthdetails-tabs-mahadasha-tab" data-bs-toggle="pill" data-toggle="pill" href="#birthdetails-tabs-mahadasha" role="tab" aria-controls="birthdetails-tabs-mahadasha" aria-selected="false"> Mahadasha </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" id="birthdetails-tabs-date-finder-tab" data-bs-toggle="pill" data-toggle="pill" href="#birthdetails-tabs-date-finder" role="tab" aria-controls="birthdetails-tabs-date-finder" aria-selected="false"> Date Finder </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link " id="birthdetails-tabs-best-kundali-dates-tab" data-bs-toggle="pill" data-toggle="pill" href="#birthdetails-tabs-best-kundali-dates" role="tab" aria-controls="birthdetails-tabs-best-kundali-dates" aria-selected="false"> Best Kundali Dates </a>
                            </li>
                        </ul>
                    </div>
                    <div class="card-body">
                        <div class="tab-content" id="birthdetails-tabs-tabContent">
                            @* Charts *@
                            <div class="tab-pane fade active show" id="birthdetails-tabs-charts" role="tabpanel" aria-labelledby="birthdetails-tabs-charts-tab">
                                <div class="row">
                                    <div class="col-lg-7 mb-3 mb-lg-0">
                                        <div class="chart-table table-responsive">
                                            <table class="table">

                                                <thead class="p-0 bg-da-yellow">
                                                    <tr>
                                                        <th>House</th>
                                                        <th>RL-NL-SL-SSL</th>
                                                        <th>Nakshatra</th>
                                                        <th>Sub Lord Nakshatra</th>
                                                    </tr>
                                                </thead>
                                                @if (ListView_House == null)
                                                {
                                                    <tbody class="data-loader">
                                                        <tr>
                                                            <td><div class="overlay"><i class="fas fa-2x fa-sync fa-spin"></i></div></td>
                                                        </tr>
                                                        @for (int i = 0; i < 12; i++)
                                                        {
                                                            <tr>
                                                                <td>1</td>
                                                                <td>सूर्य--शुक्र--शुक्र--राहु</td>
                                                                <td>| 11 | -6 7 | 11 | -6 7</td>
                                                                <td>| 11 | -6 7 | -8 | +10 3</td>
                                                            </tr>
                                                        }

                                                    </tbody>
                                                }
                                                else
                                                {
                                                    <tbody>
                                                        @foreach (var row in ListView_House ?? new List<ChartHouseTableTRModel>())
                                                        {
                                                            <tr class="@row.ForeColor @row.SelectedClass" @ondblclick="()=> OnDblClick_TR_ListView_House(row)" @onclick="()=> OnClick_TR_ListView_House(row)">
                                                                <td>@row.House</td>
                                                                <td>@row.RL_NL_SL_SSL</td>
                                                                <td>@row.NakSigni</td>
                                                                <td>@row.SubSigni</td>
                                                            </tr>
                                                        }
                                                    </tbody>
                                                }


                                            </table>
                                        </div>
                                    </div>

                                    <div class="col-lg-5">

                                        <div class="chart-table table-responsive">
                                            <table class="table">
                                                <thead class="p-0 bg-da-yellow">
                                                    <tr>
                                                        <th>Planet</th>
                                                        <th>RL-NL-SL-SSL</th>
                                                        <th>Significators</th>
                                                    </tr>
                                                </thead>
                                                @if (ListView_Planet == null)
                                                {
                                                    <tbody class="data-loader">
                                                        <tr>
                                                            <td><div class="overlay"><i class="fas fa-2x fa-sync fa-spin"></i></div></td>
                                                        </tr>
                                                        @for (int i = 0; i < 9; i++)
                                                        {
                                                            <tr>
                                                                <td>सूर्य</td>
                                                                <td>मंगल--सूर्य--चंद्र--केतु</td>
                                                                <td>| 9 | 1 | 9 | 1</td>
                                                            </tr>
                                                        }

                                                    </tbody>
                                                }
                                                else
                                                {
                                                    <tbody>
                                                        @foreach (var row in ListView_Planet ?? new List<ChartPlanetTableTRModel>())
                                                        {

